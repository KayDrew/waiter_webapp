<div class="container login">
<h2 id="logHead">Admin Login</h2>
<form action="/adminLogin" method="post">
<label for="name">username: </label><input type="text"   value="Admin" name="logName" disabled/><br/>
<label for="password">password: </label><input type="password"  name="adminPass"/>
<p class="error">{{messages.adminError}}</p>
<button type="submit">login</button>
</form>
</div>
<script>

setTimeout(function(){document.querySelector(".error").style.display="none";
},5000);

document.querySelector(".error").style.display="block";
</script>
